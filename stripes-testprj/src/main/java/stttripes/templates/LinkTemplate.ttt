<%@ page import="com.pojosontheweb.ttt.stripes.StripesTags" %>
<%!
    Class beanClass; // wildcards are not supported in generics
    String label;
    String text;
%>
<%
    StripesTags s = new StripesTags(out);
%>
<span class="my-link">
    <%= label %>
    <%= s.link(beanClass).setText(text) %>
</span>
