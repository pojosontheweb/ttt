<%--
    JSP comment can span
    several lines
    and are ignored when template gets compiled
--%>
<%@ page import="com.xyz.myapp.Foo" %>
<%@ page extends="java.io.Serializable" %>
<%!
    Foo foo;
    int bar;
%>
<div class="foo">
    <span><%= foo.getBar() %></span>
    <ul>
    <% for(int i = 0; i < bar; i++) { %>
        <li>i equals <%= i %> </li>
    <% } %>
    </ul>
</div>